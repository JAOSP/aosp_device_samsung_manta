[receive]
	requireContributorAgreement = true
[submit]
	action = merge always
	mergeContent = true
[project]
[access]
	inheritFrom = Platform-Unrestricted-Projects
[access "refs/heads/idea133-weekly-release"]
	exclusiveGroupPermissions = read forgeAuthor forgeCommitter forgeServerAsCommitter push pushMerge submit
	read = group ldap/android-git
	read = group mdb/android-git
	push = group ldap/android-git
	push = +force group mdb/android-git
	submit = group ldap/android-git
	submit = group mdb/android-git
	owner = group ldap/android-git
	owner = group mdb/android-git
	forgeAuthor = group ldap/android-git
	forgeAuthor = group mdb/android-git
	forgeCommitter = group ldap/android-git
	forgeCommitter = group mdb/android-git
	forgeServerAsCommitter = group ldap/android-git
	forgeServerAsCommitter = group mdb/android-git
	pushMerge = group ldap/android-git
	pushMerge = group mdb/android-git
